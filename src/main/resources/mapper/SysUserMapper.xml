    <!--查询每个部门人数-->
    <select id="findPieData" resultType="java.util.Map">
        SELECT COUNT(*) value,d.`name` name
        FROM sys_user u
                 INNER JOIN sys_dept d
        WHERE d.`id` = u.`dept_id`
        GROUP BY u.`dept_id`
    </select>

    <!--统计每个部门男女员工的数量-->
    <select id="findBarData" resultType="java.util.Map">
        SELECT d.`name` NAME,
               SUM(CASE sex WHEN '男' THEN 1 ELSE 0 END) boy,
               SUM(CASE sex WHEN '女' THEN 1 ELSE 0 END) girl
        FROM sys_user u
                 INNER JOIN sys_dept d
                            ON u.`dept_id` = d.`id`
        GROUP BY u.`dept_id`
    </select>

